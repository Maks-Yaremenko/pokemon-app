{
  "npmScope": "pokemon-workspace",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "pokemon-app": {
      "tags": ["scope:pokemon-app"],
      "implicitDependencies": ["pokemon-data-access", "pokemon-ui"]
    },
    "pokemon-app-e2e": {
      "tags": ["scope:pokemon-app"],
      "implicitDependencies": ["pokemon-app"]
    },
    "pokemon-api": {
      "tags": ["scope:pokemon-api"],
      "implicitDependencies": ["pokemon-models"]
    },
    "pokemon-data-access": {
      "tags": ["scope:pokemon-data-access"],
      "implicitDependencies": ["pokemon-models"]
    },
    "pokemon-ui": {
      "tags": ["scope:pokemon-ui"],
      "implicitDependencies": []
    },
    "pokemon-models": {
      "tags": ["scope:pokemon-models"],
      "implicitDependencies": []
    }
  }
}
